<script src="https://js.chargebee.com/v2/chargebee.js"></script>
<script>
		const cbInstance = window.Chargebee.init({site: "<%= ENV['CHARGEBEE_SITE'] %>"});
		let checkout_step = "";
		let monthly_price = 10;
		let annually_price = 8;

		$('a.membership').on('click', function() {
				if ($(this).data('plan') == 'free') {
						openCheckout('monthly', 'free', 1);
				}
				else {
						$('input#membership_plan').val($(this).data('plan'));
						$('input#seat_nums').val(<%=current_client.resources.active.count %>);
						if ($(this).data('plan') == 'basic') {
							monthly_price = 10;
							annually_price = 8;
						}

						if ($(this).data('plan') == 'premium') {
							monthly_price = 15;
							annually_price = 12;
						}

						$('span#monthly_price').html(monthly_price);
						$('span#monthly_amount').html(monthly_price);
						$('span#annually_price').html(annually_price);
						$('span#annually_amount').html(annually_price * 12)
						$('span#annually_save_amount').html((monthly_price - annually_price) * 12);
				}
		})
		$('input#seat_nums').on('keyup', function() {
				$('span#monthly_amount').html(monthly_price * $(this).val());
				$('span#annually_amount').html(annually_price * $(this).val() * 12);
				$('span#annually_save_amount').html((monthly_price - annually_price) * 12 * $(this).val());
		})
		$('input#seat_nums').on('change', function() {
			$('span#monthly_amount').html(monthly_price * $(this).val());
			$('span#annually_amount').html(annually_price * $(this).val() * 12);
			$('span#annually_save_amount').html((monthly_price - annually_price) * 12 * $(this).val());
		})

		$('button#upgrade-plan').on('click', function(e) {
				e.preventDefault();
				const upgrade_button = document.getElementById('upgrade-plan');
				upgrade_button.disabled = true;
				upgrade_button.setAttribute('data-kt-indicator', 'on');

				const membership_period = $('input#user_membership_monthly').prop('checked') ? 'monthly' : 'annually';
				openCheckout(membership_period, $('input#membership_plan').val(), $('input#seat_nums').val())
		})

		function openCheckout(period, plan, seat_nums) {
				cbInstance.openCheckout({
						hostedPage: function() {
								// Hit your end point that returns hosted page object as response
								// This sample end point will call the below api
								// You can pass hosted page object created as a result of checkout_new, checkout_existing, manage_payment_sources
								// https://apidocs.chargebee.com/docs/api/hosted_pages#checkout_new_subscription
								// https://apidocs.chargebee.com/docs/api/hosted_pages#checkout_existing_subscription
								// https://apidocs.chargebee.com/docs/api/hosted_pages#manage_payment_sources
								// If you want to use paypal, go cardless and plaid, pass embed parameter as false
								const membership_period = $('input#user_membership_monthly').prop('checked') ? 'monthly' : 'annually';
								return $.ajax({
										method: "post",
										url: "<%= billing_client_path %>",
										data: {
												'membership_period': period,
												'membership_plan': plan,
												'seat_nums' : seat_nums
										}
								});
						},
						loaded: function() {
								console.log("checkout opened");
						},
						error: function() {
						},
						close: function() {
								console.log("checkout closed");
								if (checkout_step == 'thankyou_screen') {
										if (plan == 'free')
												window.location.href = $('input#client_billing_path').val();
										else
												window.location.href = "<%= aggregates_reports_path %>";
								}
								else {
										const upgrade_button = document.getElementById('upgrade-plan');
										upgrade_button.disabled = false;
										upgrade_button.removeAttribute('data-kt-indicator');
										$('div#kt_modal_upgrade_to').modal('hide');
								}

						},
						success: function(hostedPageId) {
								console.log('success');
								console.log(hostedPageId);
								// Hosted page id will be unique token for the checkout that happened
								// You can pass this hosted page id to your backend
								// and then call our retrieve hosted page api to get subscription details
								// https://apidocs.chargebee.com/docs/api/hosted_pages#retrieve_a_hosted_page
						},
						step: function(value) {
								// value -> which step in checkout
								console.log('step = ' + value);
								checkout_step = value;

						}
				});
		}
</script>